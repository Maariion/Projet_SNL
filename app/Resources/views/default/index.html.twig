{% extends 'base.html.twig' %}
{% block body %}
    <div class="row">
        <div class="col s12 m6">
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title">Card Title</span>
                    <p>I am a very simple card. I am good at containing small bits of information.
                        I am convenient because I require little markup to use effectively.</p>
                </div>
                <div class="card-action">
                    <a href="{{ path('softnlabs_client_part')}}">This is a link</a>
                    <a href="{{ path('client')}}">This is another link</a>
                </div>
            </div>
        </div>
    </div>
    <div class = "card-panel">
        <div class="row">
            <form class="col s12">
                <div class="row">
                    <div class="input-field col s3">
                    </div>
                    <div class="input-field col s6">
                        <i class="material-icons prefix">account_circle</i>
                        <input id="icon_prefix" type="text" class="validate">
                        <label for="icon_prefix">Addresse e-mail</label>
                    </div>
                    <div class="input-field col s3">
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s3">
                    </div>
                    <div class="input-field col s6">
                        <i class="material-icons prefix">vpn_key</i>
                        <input id="vpn_key" type="password" class="validate">
                        <label for="vpn_key">Mot de passe</label>
                    </div>
                    <div class="input-field col s3">
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s3">
                    </div>
                    <div class="input-field col s6">
                        <div class="center-align">
                            <button class="btn waves-effect waves-light" type="submit" name="action">Se connecter
                                <i class="material-icons right">arrow_forward</i>
                            </button>
                        </div>
                    </div>
                    <div class="input-field col s3">
                    </div>
                </div>
            </form>
        </div>
    </div>
{% endblock %}


