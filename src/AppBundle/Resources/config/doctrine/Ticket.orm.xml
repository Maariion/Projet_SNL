<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ticket" table="ticket">
    <indexes>
      <index name="Ticket_Criticite" columns="IDCrit"/>
      <index name="Ticket_Statut" columns="IDSta"/>
      <index name="Ticket_Systeme" columns="IDSys"/>
      <index name="Ticket_Type" columns="IDTyp"/>
      <index name="Ticket_Utilisateur_Client" columns="IDUtil_Consultant"/>
      <index name="Ticket_Utilisateur_Consultant" columns="IDUtil_Client"/>
    </indexes>
    <id name="idtick" type="integer" column="IDTick">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="Titre" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="Description" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tpsprisecompte" type="datetime" column="TpsPriseCompte" nullable="true"/>
    <field name="tpsresolution" type="datetime" column="TpsResolution" nullable="true"/>
    <field name="idtyp" type="integer" column="IDTyp" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idsta" type="integer" column="IDSta" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idcrit" type="integer" column="IDCrit" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idutilClient" type="integer" column="IDUtil_Client" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idutilConsultant" type="integer" column="IDUtil_Consultant" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idsys" type="integer" column="IDSys" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
