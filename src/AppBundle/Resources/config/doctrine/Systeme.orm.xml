<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Systeme" table="systeme">
    <id name="idsys" type="integer" column="IDSys">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="version" type="string" column="Version" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nom" type="string" column="Nom" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="editeur" type="string" column="Editeur" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
