<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Utilisateur" table="utilisateur">
    <indexes>
      <index name="Utilisateur_Organisation" columns="Organisation_ID"/>
    </indexes>
    <id name="mail" type="string" column="Mail" length="50">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="Nom" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="Prenom" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="role" type="string" column="Role" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="actif" type="boolean" column="Actif" nullable="false"/>
    <field name="motpasse" type="string" column="MotPasse" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="organisationId" type="integer" column="Organisation_ID" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
